{
  "name": "node-express-cli",
  "version": "3.0.5",
  "description": "Simple express cli generator",
  "main": "dist/index.js",
  "bin": {
    "node-express-cli": "dist/index.js",
    "nec": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index",
    "prepare": "npm run build",
    "prettier:fix": "prettier --config .prettierrc.json --write src/**/**/*.ts && prettier --config .prettierrc.json"
  },
  "keywords": [
    "express",
    "cli",
    "generator"
  ],
  "author": "Fernando Acosta",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/FernandoAcTr/node-express-cli"
  },
  "dependencies": {
    "@inquirer/prompts": "^5.3.6",
    "colors": "^1.4.0",
    "figlet": "^1.7.0",
    "fs-extra": "^11.3.0",
    "gradient-string": "^2.0.2",
    "shelljs": "^0.8.5",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/figlet": "^1.5.8",
    "@types/fs-extra": "^11.0.4",
    "@types/gradient-string": "^1.1.2",
    "@types/shelljs": "^0.8.15",
    "@types/yargs": "^17.0.32"
  }
}
